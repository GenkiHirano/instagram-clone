<%= form_tag(search_path,:method => 'get') do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :content => nil %>
<% end %>

<% if @posts.present? %>
  <% @posts.each do |post| %>
  <% binding.pry %>

    <%= post.content %><br>
    <%= post.image %><br>
    <%= post.id %><br>
    <%= post.created_at %><br>
    <%= post.updated_at %><br>

  <% end %>
<% end %>

<br>
